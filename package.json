{
  "name": "datav-vue",
  "version": "0.0.1",
  "description": "A Powerful Data Visualization Tool",
  "scripts": {
    "gc": "bash ./scripts/gc.sh",
    "new": "plop --plopfile ./plop-templates/plopfile.js",
    "bootstrap": "yarn --frozen-lockfile",
    "dev": "cross-env NODE_ENV=development vite",
    "build": "NODE_ENV=production vue-tsc --noEmit && vite build",
    "build:fast": "NODE_ENV=production vite build",
    "serve": "vite preview",
    "lint": "yarn lint:type && yarn lint:code && yarn lint:css",
    "lint:type": "vue-tsc --noEmit",
    "lint:code": "eslint --fix 'src/**/*.{js,ts,vue}'",
    "lint:css": "stylelint --fix 'src/**/*.(css|scss|vue)'",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "prepare": "husky install"
  },
  "dependencies": {
    "@monaco-editor/loader": "^1.2.0",
    "accounting": "^0.4.1",
    "axios": "^0.21.1",
    "axios-mock-adapter": "^1.19.0",
    "crypto-js": "^4.0.0",
    "dayjs": "^1.10.7",
    "echarts": "^5.2.2",
    "echarts-wordcloud": "^2.0.0",
    "element-plus": "1.0.2-beta.55",
    "gsap": "^3.7.1",
    "html2canvas": "^1.3.3",
    "js-cookie": "^3.0.1",
    "lodash-es": "^4.17.21",
    "mockjs": "^1.1.0",
    "monaco-editor": "0.27.0",
    "naive-ui": "^2.21.5",
    "number-precision": "^1.5.1",
    "particles.vue3": "^1.20.5",
    "shortid": "^2.2.16",
    "vue": "^3.2.24",
    "vue-echarts": "^6.0.0",
    "vue-i18n": "^9.1.9",
    "vue-router": "^4.0.12",
    "vuex": "^4.0.2",
    "vuex-module-decorators": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@types/accounting": "^0.4.2",
    "@types/crypto-js": "^4.0.1",
    "@types/lodash-es": "^4.17.4",
    "@types/mockjs": "^1.0.3",
    "@types/shortid": "^0.0.29",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "@vitejs/plugin-vue": "^1.10.2",
    "@vue/compiler-sfc": "^3.2.24",
    "@vue/component-compiler-utils": "^3.3.0",
    "conventional-changelog-cli": "^2.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.4.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-vue": "^8.2.0",
    "handlebars": "^4.7.7",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "plop": "^2.7.4",
    "postcss": "^8.4.5",
    "postcss-html": "^1.3.0",
    "postcss-scss": "^4.0.2",
    "rimraf": "^3.0.2",
    "sass": "^1.45.0",
    "stylelint": "^14.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^24.0.0",
    "typescript": "^4.5.4",
    "vite": "2.7.1",
    "vite-plugin-plain-text": "^1.1.4",
    "vue-tsc": "^0.29.8"
  },
  "lint-staged": {
    "src/**/*.{js,ts,vue}": [
      "eslint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:pengxiaotian/datav-vue.git"
  },
  "homepage": "https://datav.pengxiaotian.com",
  "author": "Xiaotian (Allen) Peng",
  "license": "MIT",
  "browserslist": [
    "> 2%",
    "last 2 versions",
    "not IE 11"
  ]
}
